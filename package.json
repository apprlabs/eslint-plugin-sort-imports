{
  "name": "@apprlabs/eslint-plugin-sort-imports",
  "version": "0.0.2",
  "description": "ESLint plugin to sort imports by configurable sections with optional comments.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "prettier -c src/**/*.ts",
    "format:fix": "prettier --write src/**/*.ts",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apprlabs/eslint-plugin-sort-imports.git"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "sort-imports"
  ],
  "author": "Appr Labs",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/parser": "^8.18.0",
    "@typescript-eslint/rule-tester": "^8.18.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0"
  },
  "peerDependencies": {
    "eslint": "^9.17.0"
  }
}
